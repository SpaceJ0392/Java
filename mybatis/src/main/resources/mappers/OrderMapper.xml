<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="spring_study.mybatis.mapper.OrderMapper">
    <insert id="insertOrder" parameterType="spring_study.mybatis.dto.Order">
        INSERT INTO ORDERS (user_id, product_id, quantity) VALUES (#{userId}, #{productId}, #{quantity});
    </insert>
</mapper>